{
  "name": "oops",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "git@github.com:ctrlup-io/oops.git",
  "author": "Valentin Barit <valentin.barit@gmail.com>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "eslint": "^8.12.0",
    "git-cz": "^4.8.0",
    "husky": "^7.0.4",
    "lint-staged": ">=10"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "env": {
      "es6": true
    },
    "parserOptions": {
